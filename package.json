{
  "name": "lynx-website",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}\"",
    "check:format": "npm run prettier -- --check",
    "check:types": "tsc --project tsconfig.json",
    "check:spelling": "cspell --show-context --no-progress \"src/**/*\" \"**.md\"",
    "check:lint": "next lint",
    "check:build": "next build && next export",
    "fix:format": "npm run prettier -- --write",
    "fix:lint": "npm run check:lint -- --fix",
    "validate": "run-script-os",
    "validate:darwin:linux": "FORCE_COLOR=1 npm-run-all --parallel --aggregate-output --print-label check:*",
    "validate:win32": "npm-run-all --parallel --aggregate-output --print-label check:*"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "@mui/styled-engine": "^5.2.6",
    "@mui/styles": "^5.3.0",
    "@next/mdx": "^14.0.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/mdx": "^2.0.10",
    "axios": "^0.25.0",
    "chroma-js": "^2.1.2",
    "flatpickr": "^4.6.9",
    "next": "^12.3.4",
    "next-compose-plugins": "^2.2.1",
    "nodemailer": "^6.7.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-countup": "^6.1.0",
    "react-dom": "^17.0.2",
    "react-flatpickr": "^3.10.7"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cspell": "^5.16.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "run-script-os": "^1.1.6",
    "typescript": "^4.5.4"
  }
}
